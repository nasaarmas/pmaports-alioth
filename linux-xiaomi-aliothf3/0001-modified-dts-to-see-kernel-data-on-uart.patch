From 79f466010e25771472fc761df3c94f3470c1107a Mon Sep 17 00:00:00 2001
From: Bartek G <bartek.glocki@tlen.pl>
Date: Tue, 7 Oct 2025 14:41:32 +0200
Subject: [PATCH] modified dts to see kernel data on uart

---
 .../boot/dts/qcom/sm8250-xiaomi-alioth.dts    | 21 ++++---------------
 1 file changed, 4 insertions(+), 17 deletions(-)

diff --git a/arch/arm64/boot/dts/qcom/sm8250-xiaomi-alioth.dts b/arch/arm64/boot/dts/qcom/sm8250-xiaomi-alioth.dts
index 4652c6241f38..558faa4ddf3c 100644
--- a/arch/arm64/boot/dts/qcom/sm8250-xiaomi-alioth.dts
+++ b/arch/arm64/boot/dts/qcom/sm8250-xiaomi-alioth.dts
@@ -39,13 +39,14 @@ / {
 	qcom,board-id = <44 0>;
 
 	aliases {
-		serial0 = &uart6;
+		serial0 = &uart12;
 	};
 
 	chosen {
 		#address-cells = <2>;
 		#size-cells = <2>;
 		ranges;
+        stdout-path = "serial0:115200n8";
 	};
 
 	battery: battery {
@@ -1278,22 +1279,8 @@ mclk0-pins {
 	};
 };
 
-&uart6 {
+&uart12 {
 	status = "okay";
-
-	bluetooth {
-		compatible = "qcom,qca6390-bt";
-		pinctrl-names = "default";
-		pinctrl-0 = <&sw_ctrl>;
-		swctrl-gpios = <&tlmm 124 GPIO_ACTIVE_HIGH>;
-		vddrfacmn-supply = <&vreg_pmu_rfa_cmn>;
-		vddaon-supply = <&vreg_pmu_aon_0p59>;
-		vddasd-supply = <&vreg_l16a_3p3>;
-		vddbtcmx-supply = <&vreg_pmu_btcmx_0p85>;
-		vddrfa0p8-supply = <&vreg_pmu_rfa_0p8>;
-		vddrfa1p2-supply = <&vreg_pmu_rfa_1p2>;
-		vddrfa1p7-supply = <&vreg_pmu_rfa_1p7>;
-	};
 };
 
 &usb_1 {
@@ -1341,4 +1328,4 @@ &ufs_mem_phy {
 &venus {
 	firmware-name = "qcom/sm8250/xiaomi/alioth/venus.mbn";
 	status = "okay";
-};
\ No newline at end of file
+};
